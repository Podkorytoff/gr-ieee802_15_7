<?xml version="1.0"?>

<block>
	<name>Generate PHR</name>
	<key>ieee802_15_7_generate_PHR</key>
	<category>IEEE802.15.7</category>
	<import>import ieee802_15_7</import>
	<make>ieee802_15_7.generate_PHR($debug)</make>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>

	<source>
		<name>out</name>
		<type>byte</type>
		<nports>1</nports>
	</source>

</block>
